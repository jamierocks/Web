{% extends 'KorobiWebBundle::layout.html.twig' %}

{% set page_title = "Docs" %}

{% block head %}
    <base href="{{ url('docs') }}/">
{% endblock %}

{% block body %}
    {% if pageName != "index" %}
        <a href="{{ path('docs') }}">Return to docs index</a>
    {% endif %}
    <section class="docs">
        {{ content | raw }}
    </section>
{% endblock %}
