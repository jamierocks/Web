<!DOCTYPE html>
<html>
<head>
    <title>{% if page_title is defined %}{{ page_title }} - {% endif %}{{ app_name }}</title>
    <meta name='viewport' content='initial-scale=1'>
    {% stylesheets filter='scss' '@KorobiWebBundle/Resources/assets/sass/application.scss' %}
    <link rel='stylesheet' href='{{ asset_url }}' />
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/font-awesome/4.3.0/css/font-awesome.min.css">
    {% endstylesheets %}
</head>
<body>
{{ include('KorobiWebBundle::partials/nav.html.twig') }}
<main>
    {% block body %}{% endblock %}
</main>

{% javascripts '@KorobiWebBundle/Resources/assets/js/*' output='assets/js/application.js' %}
<script src="{{ asset_url }}"></script>
{% endjavascripts %}
{# <script src='{{ asset('assets/js/application.js') }}'></script> #}
</body>
</html>
