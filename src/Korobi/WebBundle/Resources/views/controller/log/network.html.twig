{% extends 'KorobiWebBundle::layout.html.twig' %}

{% set page_title = "Log" %}

{% block body %}
    <h1>Logged channels on {{ network }}</h1>
    <table cellpadding="0" cellspacing="0">
        <tbody>
        <tr>
            <th>Channel</th>
            <th>Last Activity</th>
        </tr>
        {% for channel in channels %}
        <tr>
            <td><a href="{{ channel.href }}">{{ channel.name }}</a></td>
            <td>Unknown</td>
        </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
